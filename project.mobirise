    "Quito.html": {
      "settings": {
        "meta_descr": "Web Generator Description",
        "title": "Quito",
        "order": 1
      },
      "components": [
        {
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 15px)",
              "padding-bottom": "(@paddingBottom * 15px)"
            },
